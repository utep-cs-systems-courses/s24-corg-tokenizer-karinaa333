# compiler flags. -g for debug, -03 for optimization
CFLAGS = -g -03

# object files
OBJS = uimain.o tokenizer.o history.o

# include file dependencies
tokenizer.o: tokenizer.h
history.o: history.h

all: tokenize
   cc -o tokenize $(CFLAGS) $(OBJS)

# delete files generated by compilation
clean:
	rm -f *.o tokenize
run: tokenize
	./tokenize
